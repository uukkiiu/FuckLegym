<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="list_pref_guideline_begin">56dp</dimen>
    <string name="cancel">取消</string>
    <string name="confirm">确定</string>
    <declare-styleable name="BaselineGridTextView">
        <attr format="float" name="lineHeightMultiplierHint"/>
        <attr format="dimension" name="lineHeightHint"/>
        <attr format="boolean" name="maxLinesByHeight"/>
        <attr name="android:textAppearance"/>
        <attr name="android:fontFamily"/>
    </declare-styleable>
    <declare-styleable name="CommonPreference">
        <attr format="string" name="endText"/>
        <attr format="boolean" name="showRightArrowIcon"/>
    </declare-styleable>
    <declare-styleable name="CommonSettingsView">
        <attr format="string" name="title"/>
        <attr format="string" name="subtitle"/>
        <attr format="string" name="end_text"/>
        <attr format="reference" name="icon"/>
        <attr format="boolean" name="showEndIcon"/>
    </declare-styleable>
    <declare-styleable name="DontAskAgainListPreference">
        <attr format="boolean" name="enableDontAskAgain"/>
    </declare-styleable>
</resources>